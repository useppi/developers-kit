<?xml version="1.0" encoding="UTF-8"?>
<!--
Example for a mandatory error for overlapping periods in a category OTA_HotelAvailNotifRQ message.
-->
<OTA_HotelAvailNotifRQ xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                       xmlns="http://www.opentravel.org/OTA/2003/05"
                       xsi:schemaLocation="http://www.opentravel.org/OTA/2003/05 OTA_HotelAvailNotifRQ.xsd"
                       Version="1.002">
    <UniqueID Type="16" ID="1" Instance="CompleteSet"/>
    <AvailStatusMessages HotelCode="{HOTELCODE}">
        <AvailStatusMessage BookingLimit="3" BookingLimitMessageType="SetLimit">
            <StatusApplicationControl Start="{YEAR}-08-01" End="{YEAR}-08-10" InvTypeCode="EZ_1" />
        </AvailStatusMessage>
        <AvailStatusMessage BookingLimit="1" BookingLimitMessageType="SetLimit">
            <StatusApplicationControl Start="{YEAR}-08-10" End="{YEAR}-08-30" InvTypeCode="EZ_1" />
        </AvailStatusMessage>
    </AvailStatusMessages>
</OTA_HotelAvailNotifRQ>
